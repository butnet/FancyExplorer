<configuration>
    <configSections>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
            <section name="FancyExplorer.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
        </sectionGroup>
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <userSettings>
        <FancyExplorer.Properties.Settings>
            <setting name="shortcutKeyGesture" serializeAs="String">
                <value>Ctrl+D</value>
            </setting>
            <setting name="windowHeightPercent" serializeAs="String">
                <value>0.5</value>
            </setting>
            <setting name="animFramerate" serializeAs="String">
                <value>200</value>
            </setting>
            <setting name="animDuration" serializeAs="String">
                <value>0.2</value>
            </setting>
            <setting name="autosavedTabs" serializeAs="String">
                <value />
            </setting>
        </FancyExplorer.Properties.Settings>
    </userSettings>
</configuration>